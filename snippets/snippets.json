{
	"nc": {
		"description": "newcommand",
		"prefix": "nc",
		"body": "\\newcommand{\\${1:cmd}}[${2:opt}]{${3:realcmd}} ${0}",
		"scope": "text.tex.latex"
	},
	"up": {
		"description": "usepackage",
		"prefix": "up",
		"body": "\\usepackage[${1:options}]{${2:package}} ${0}",
		"scope": "text.tex.latex"j
	},
	"nuc": {
		"description": "newunicodechar",
		"prefix": "nuc",
		"body": "\\newunicodechar{${1}}{${2:\\ensuremath}${3:tex-substitute}}} ${0}",
		"scope": "text.tex.latex"
	},
	"dmo": {
		"description": "DeclareMathOperator",
		"prefix": "dmo",
		"body": "\\DeclareMathOperator{${1}}{${2}} ${0}",
		"scope": "text.tex.latex"
	},
	"begin": {
		"description": "\\begin{}...\\end{} block",
		"prefix": "begin",
		"body": "\\begin{${1:env}}\n\t${0}\n\\end{$1}",
		"scope": "text.tex.latex"
	},
	"tab": {
		"description": "Tabular",
		"prefix": "tab",
		"body": "\\begin{${1:tabular}}{${2:c}}\n\t${0}\n\\end{$1}",
		"scope": "text.tex.latex"
	},
	"thm": {
		"description": "thm (or arbitrary) environment with optional argument",
		"prefix": "thm",
		"body": "\\begin[${1:author}]{${2:thm}}\n\t${0}\n\\end{$2}",
		"scope": "text.tex.latex"
	},
	"center": {
		"prefix": "center",
		"description": "center environment",
		"body": "\\begin{center}\n\t${0}\n\\end{center}",
		"scope": "text.tex.latex"
	},
	"ali": {
		"description": "align(ed) environment",
		"prefix": "ali",
		"body": "\\begin{align${1:ed}}\n\t\\label{eq:${2}}\n\t${0}\n\\end{align$1}",
		"scope": "text.tex.latex"
	},
	"gat": {
		"description": "gather(ed) environment",
		"prefix": "gat",
		"body": "\\begin{gather${1:ed}}\n\t${0}\n\\end{gather$1}",
		"scope": "text.tex.latex"
	},
	"eq": {
		"description": "Equation",
		"prefix": "eq",
		"body": "\\begin{equation}\n\t${0}\n\\end{equation}",
		"scope": "text.tex.latex"
	},
	"eql": {
		"description": "Labeled equation environment",
		"prefix": "eql",
		"body": "\\begin{equation}\n\t\\label{eq:${2}}\n\t${0}\n\\end{equation}",
		"scope": "text.tex.latex"
	},
	"eq*": {
		"description": "unnumbered equation environment",
		"prefix": "eq*",
		"body": "\\begin{equation*}\n\t${0}\n\\end{equation*}",
		"scope": "text.tex.latex"
	},
	"r[": {
		"description": "unnumbered equation: \\[ ... \\]",
		"prefix": "r[",
		"body": "\\[\n\t${0}\n\\]",
		"scope": "text.tex.latex"
	},
	"eqnarray": {
		"description": "Equation array",
		"prefix": "eqnarray",
		"body": "\\begin{eqnarray}\n\t${0}\n\\end{eqnarray}",
		"scope": "text.tex.latex"
	},
	"lab": {
		"description": "Label",
		"prefix": "lab",
		"body": "\\label{${1:eq:}${2:fig:}${3:tab:}${0}}",
		"scope": "text.tex.latex"
	},
	"enum": {
		"description": "enumerate environment",
		"prefix": "enum",
		"body": "\\begin{enumerate}\n\t\\item ${0}\n\\end{enumerate}",
		"scope": "text.tex.latex"
	},
	"enuma": {
		"description": "enumerate environment",
		"prefix": "enuma",
		"body": "\\begin{enumerate}[(a)]\n\t\\item ${0}\n\\end{enumerate}",
		"scope": "text.tex.latex"
	},
	"enuml": {
		"description": "enumerate environment",
		"prefix": "enumi",
		"body": "\\begin{enumerate}[(i)]\n\t\\item ${0}\n\\end{enumerate}",
		"scope": "text.tex.latex"
	},
	"itemize": {
		"description": "itemize environment",
		"prefix": "itemize",
		"body": "\\begin{itemize}\n\t\\item ${0}\n\\end{itemize}"
	},
	"item": {
		"prefix": "item",
		"body": "\\item ${1}",
		"scope": "text.tex.latex"
	},
	"desc": {
		"description": "Description environment",
		"prefix": "desc",
		"body": "\\begin{description}\n\t\\item[${1}] ${0}\n\\end{description}",
		"scope": "text.tex.latex"
	},
	"]i": {
		"description": "Endless new item",
		"prefix": "]i",
		"body": "\\item ${1}\n\t${0:]i}",
		"scope": "text.tex.latex"
	},
	"mat": {
		"description": "smart matrix environment",
		"prefix": "mat",
		"body": "\\begin{${1:p/b/v/V/B/small}matrix}\n\t${0}\n\\end{$1matrix}",
		"scope": "text.tex.latex"
	},
	"cas": {
		"description": "cases environment",
		"prefix": "cas",
		"body": "\\begin{cases}\n\t${1:equation}, &\\text{ if }${2:case}\\\n\t${0}\n\\end{cases}",
		"scope": "text.tex.latex"
	},
	"spl": {
		"description": "split environment",
		"prefix": "spl",
		"body": "\\begin{split}\n\t${0}\n\\end{split}",
		"scope": "text.tex.latex"
	},
	"part": {
		"description": "document \\part",
		"prefix": "part",
		"body": "\\part{${1:part name}} % (fold)\n\\label{prt:${2:$1}}\n${0}\n% part $2 (end)",
		"scope": "text.tex.latex"
	},
	"cha": {
		"description": "Chapter",
		"prefix": "cha",
		"body": "\\chapter{${1:chapter name}}\n\\label{cha:${2:$1}}\n${0}",
		"scope": "text.tex.latex"
	},
	"sec": {
		"description": "Section",
		"prefix": "sec",
		"body": "\\section{${1:section name}}\n\\label{sec:${2:$1}}\n${0}",
		"scope": "text.tex.latex"
	},
	"sec*": {
		"description": "Section without number",
		"prefix": "sec*",
		"body": "\\section*{${1:section name}}\n\\label{sec:${2:$1}}\n${0}",
		"scope": "text.tex.latex"
	},
	"sub": {
		"description": "Sub Section",
		"prefix": "sub",
		"body": "\\subsection{${1:subsection name}}\n\\label{sub:${2:$1}}\n${0}",
		"scope": "text.tex.latex"
	},
	"sub*": {
		"description": "Sub Section without number",
		"prefix": "sub*",
		"body": "\\subsection*{${1:subsection name}}\n\\label{sub:${2:$1}}\n${0}",
		"scope": "text.tex.latex"
	},
	"subs": {
		"description": "Sub Sub Section",
		"prefix": "subs",
		"body": "\\subsubsection{${1:subsubsection name}}\n\\label{ssub:${2:$1}}\n${0}",
		"scope": "text.tex.latex"
	},
	"subs*": {
		"description": "Sub Sub Section without number",
		"prefix": "subs*",
		"body": "\\subsubsection*{${1:subsubsection name}}\n\\label{ssub:${2:$1}}\n${0}",
		"scope": "text.tex.latex"
	},
	"par": {
		"description": "Paragraph",
		"prefix": "par",
		"body": "\\paragraph{${1:paragraph name}}\n\\label{par:${2:$1}}\n${0}",
		"scope": "text.tex.latex"
	},
	"subp": {
		"description": "Sub Paragraph",
		"prefix": "subp",
		"body": "\\subparagraph{${1:subparagraph name}}\n\\label{subp:${2:$1}}\n${0}"
	},
	"ni": {
		"description": "\\noindent",
		"prefix": "ni",
		"body": "\\noindent\n${0}",
		"scope": "text.tex.latex"
	},
	"itd": {
		"description": "description \\item",
		"prefix": "itd",
		"body": "\\item[${1:description}] ${0:item}",
		"scope": "text.tex.latex"
	},
	"figure": {
		"description": "reference to a figure",
		"prefix": "figure",
		"body": "${1:Figure}~\\ref{${2:fig:}}",
		"scope": "text.tex.latex"
	},
	"table": {
		"description": "reference to a table",
		"prefix": "table",
		"body": "${1:Table}~\\ref{${2:tab:}}",
		"scope": "text.tex.latex"
	},
	"listing": {
		"description": "reference to a listing",
		"prefix": "listing",
		"body": "${1:Listing}~\\ref{${2:list}}",
		"scope": "text.tex.latex"
	},
	"section": {
		"description": "reference to a section",
		"prefix": "section",
		"body": "${1:Section}~\\ref{sec:${2}} ${0}",
		"scope": "text.tex.latex"
	},
	"page": {
		"description": "reference to a page",
		"prefix": "page",
		"body": "${1:page}~\\pageref{${2}} ${0}",
		"scope": "text.tex.latex"
	},
	"index": {
		"description": "\\index",
		"prefix": "index",
		"body": "\\index{${1:index}} ${0}",
		"scope": "text.tex.latex"
	},
	"citen": {
		"description": "Citations",
		"prefix": "citen",
		"body": "\\citen{${1}} ${0}",
		"scope": "text.tex.latex"
	},
	"citep": {
		"description": "\\citep citations",
		"prefix": "citep",
		"body": "\\citep{${1}} ${0}",
		"scope": "text.tex.latex"
	},
	"citet": {
		"description": "\\citet citations",
		"prefix": "citet",
		"body": "\\citet{${1}} ${0}",
		"scope": "text.tex.latex"
	},
	"cite": {
		"description": "\\cite[]{} citations",
		"prefix": "cite",
		"body": "\\cite[${1}]{${2}} ${0}",
		"scope": "text.tex.latex"
	},
	"citea": {
		"description": "\\citeauthor citations",
		"prefix": "citea",
		"body": "\\citeauthor{${1}} ${0}",
		"scope": "text.tex.latex"
	},
	"citey": {
		"description": "\\citeyear citations",
		"prefix": "citey",
		"body": "\\citeyear{${1}} ${0}",
		"scope": "text.tex.latex"
	},
	"fcite": {
		"description": "\\footcite[]{} citations",
		"prefix": "fcite",
		"body": "\\footcite[${1}]{${2}}${0}",
		"scope": "text.tex.latex"
	},
	"it": {
		"description": "italic text",
		"prefix": "it",
		"body": "\\textit{${0:${VISUAL:text}}}",
		"scope": "text.tex.latex"
	},
	"bf": {
		"description": "bold face text",
		"prefix": "bf",
		"body": "\\textbf{${0:${VISUAL:text}}}",
		"scope": "text.tex.latex"
	},
	"under": {
		"description": "underline text",
		"prefix": "under",
		"body": "\\underline{${0:${VISUAL:text}}}",
		"scope": "text.tex.latex"
	},
	"emp": {
		"description": "emphasize text",
		"prefix": "emp",
		"body": "\\emph{${0:${VISUAL:text}}}",
		"scope": "text.tex.latex"
	},
	"sc": {
		"description": "small caps text",
		"prefix": "sc",
		"body": "\\textsc{${0:${VISUAL:text}}}",
		"scope": "text.tex.latex"
	},
	"sf": {
		"description": "sans serife text",
		"prefix": "sf",
		"body": "\\textsf{${0:${VISUAL:text}}}",
		"scope": "text.tex.latex"
	},
	"rm": {
		"description": "roman font text",
		"prefix": "rm",
		"body": "\\textrm{${0:${VISUAL:text}}}",
		"scope": "text.tex.latex"
	},
	"tt": {
		"description": "typewriter (monospace) text",
		"prefix": "tt",
		"body": "\\texttt{${0:${VISUAL:text}}}",
		"scope": "text.tex.latex"
	},
	"mf": {
		"description": "mathfrak",
		"prefix": "mf",
		"body": "\\mathfrak{${0:${VISUAL:text}}}",
		"scope": "text.tex.latex"
	},
	"mc": {
		"description": "mathcal",
		"prefix": "mc",
		"body": "\\mathcal{${0:${VISUAL:text}}}",
		"scope": "text.tex.latex"
	},
	"ms": {
		"description": "mathscr",
		"prefix": "ms",
		"body": "\\mathscr{${0:${VISUAL:text}}}",
		"scope": "text.tex.latex"
	},
	"ft": {
		"description": "\\footnote",
		"prefix": "ft",
		"body": "\\footnote{${0:${VISUAL:text}}}",
		"scope": "text.tex.latex"
	},
	"fig": {
		"description": "figure environment (includegraphics)",
		"prefix": "fig",
		"body": "\\begin{figure}\n\\begin{center}\n\t\\includegraphics[scale=${1}]{Figures/${2}}\n\\end{center}\n\\caption{${3}}\n\\label{fig:${4}}\n\\end{figure}\n${0}",
		"scope": "text.tex.latex"
	},
	"tikz": {
		"description": "figure environment (tikzpicture)",
		"prefix": "tikz",
		"body": "\\begin{figure}[htpb]\n\\begin{center}\n\\begin{tikzpicture}[scale=${1:1}, transform shape]\n\t${2}\n\\end{tikzpicture}\n\\end{center}\n\\caption{${3}}\n\\label{fig:${4}}\n\\end{figure}\n${0}",
		"scope": "text.tex.latex"
	},
	"subfig": {
		"description": "subfigure environment",
		"prefix": "subfig",
		"body": "\\begin{subfigure}[${1}]{${2:\\textwidth}}\n\\begin{center}\n\t${3}\n\\end{center}\n\\caption{${4}}\n\\label{fig:${5}}\n\\end{subfigure}\n${0}",
		"scope": "text.tex.latex"
	},
	"stackrel": {
		"description": "\\stackrel{}{}",
		"prefix": "stackrel",
		"body": "\\stackrel{${1:above}}{${2:below}} ${0}",
		"scope": "text.tex.latex"
	},
	"frac": {
		"description": "\\frac{}{}",
		"prefix": "frac",
		"body": "\\frac{${1:num}}{${2:denom}} ${0}",
		"scope": "text.tex.latex"
	},
	"sum": {
		"description": "\\sum^{}_{}",
		"prefix": "sum",
		"body": "\\sum^{${1:n}}_{${2:i=1}} ${0}",
		"scope": "text.tex.latex"
	},
	"lim": {
		"description": "\\lim_{}",
		"prefix": "lim",
		"body": "\\lim_{${1:n \\to \\infty}} ${0}",
		"scope": "text.tex.latex"
	},
	"frame": {
		"description": "frame environment",
		"prefix": "frame",
		"body": "\\begin{frame}[${1:t}]{${2:title}}\n\t${0}\n\\end{frame}",
		"scope": "text.tex.latex"
	},
	"block": {
		"description": "block environment",
		"prefix": "block",
		"body": "\\begin{block}{${1:title}}\n\t${0}\n\\end{block}",
		"scope": "text.tex.latex"
	},
	"alert": {
		"description": "alertblock environment",
		"prefix": "alert",
		"body": "\\begin{alertblock}{${1:title}}\n\t${0}\n\\end{alertblock}",
		"scope": "text.tex.latex"
	},
	"example": {
		"description": "exampleblock environment",
		"prefix": "example",
		"body": "\\begin{exampleblock}{${1:title}}\n\t${0}\n\\end{exampleblock}",
		"scope": "text.tex.latex"
	},
	"col2": {
		"description": "two-column environment",
		"prefix": "col2",
		"body": "\\begin{columns}\n\t\\begin{column}{0.5\\textwidth}\n\t\t${1}\n\t\\end{column}\n\t\\begin{column}{0.5\\textwidth}\n\t\t${0}\n\t\\end{column}\n\\end{columns}",
		"scope": "text.tex.latex"
	},
	"r{": {
		"description": "\\{ \\}",
		"prefix": "r{",
		"body": "\\{ ${0} \\}",
		"scope": "text.tex.latex"
	},
	"lr": {
		"description": "left right",
		"prefix": "lr",
		"body": "\\left${1} ${0} \\right$1",
		"scope": "text.tex.latex"
	},
	"lr(": {
		"description": "left( right)",
		"prefix": "lr(",
		"body": "\\left( ${0} \\right)",
		"scope": "text.tex.latex"
	},
	"lr|": {
		"description": "left| right|",
		"prefix": "lr|",
		"body": "\\left| ${0} \\right|",
		"scope": "text.tex.latex"
	},
	"lr{": {
		"description": "left\\{ right\\}",
		"prefix": "lr{",
		"body": "\\left\\{ ${0} \\right\\}",
		"scope": "text.tex.latex"
	},
	"lr[": {
		"description": "left[ right]",
		"prefix": "lr[",
		"body": "\\left[ ${0} \\right]",
		"scope": "text.tex.latex"
	},
	"lra": {
		"description": "langle rangle",
		"prefix": "lra",
		"body": "\\langle ${0} \\rangle",
		"scope": "text.tex.latex"
	},
	"lst": {
		"description": "Code listings",
		"prefix": "lst",
		"body": "\\begin{listing}[language=${1:language}]\n\t${0}\n\\end{listing}",
		"scope": "text.tex.latex"
	},
	"lsi": {
		"description": "Code listings",
		"prefix": "lsi",
		"body": "\\lstinline|${1}| ${0}",
		"scope": "text.tex.latex"
	},
	"url": {
		"description": "Hyperlinks",
		"prefix": "url",
		"body": "\\url{${1}} ${0}",
		"scope": "text.tex.latex"
	},
	"href": {
		"description": "Hyperlinks",
		"prefix": "href",
		"body": "\\href{${1}}{${2}} ${0}",
		"scope": "text.tex.latex"
	},
	"urlc": {
		"description": "URL from Clipboard.",
		"prefix": "urlc",
		"body": "\\url{`@+`} ${0}"
	},
	"hrefc": {
		"description": "URL from Clipboard.",
		"prefix": "hrefc",
		"body": "\\href{`@+`}{${1}} ${0}",
		"scope": "text.tex.latex"
	}
}